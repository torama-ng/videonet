{{> navbar}}

<div id="wrapper">
  {{> sidenav}}

  {{!-- <div id="page-content-wrapper"> --}}
    <div class="container align-items-center text-center pt-5 pb-2 my-5">
      <p class="lead">
        {{videoDir}} Library <br>
        # of Videos {{videoList.length}}
      </p>

      <main>
        <div id="listVideo">
          {{#each videoList}}
          <figure>
            <video controls src=" {{path}} " type="video/mp4">
            </video>
            <figcation> {{title}}</figcaption>
          </figure>
          {{/each}}
        </div>
      </main>

    </div>

    {{!-- </div> <!--  end of page content wrapper div --> --}}

  {{> footer}}

</div> <!--  end of wrapper div -->