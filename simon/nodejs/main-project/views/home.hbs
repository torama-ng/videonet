      <h1 class="mt-4">
        {{videoTitle}}
      </h1>
     <div class="row justify-content-lg-start justify-content-md-start">
      {{#each videoFiles}}

   <div class="col-md-4 col-lg-3 col-sm-12">
          <div class="card mb-3 shadow-sm">
        
          <video width="400" height="200"  class="bd-placeholder-img card-img-top" controls src="{{this}}" type="video/mp4">
          </video>
           <p class="card-text">{{{formatMe this}}}</p>
        
          </div>
      </div>
        {{/each}}
        </div>
        

         <p class="lead">
        # of Videos {{videoFiles.length}}
